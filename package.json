{
  "name": "books_api",
  "version": "1.0.0",
  "description": "This is an API about books, we offer different types of books that user can search",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "start": "npm run build && node dist/app/init/server_startUp.js",
    "start:dev": "nodemon --exec babel-node app/init/server_startUp.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "lint": "eslint --fix --env node . --ext js",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D-Rosa99/Books_API.git"
  },
  "keywords": [
    "babel",
    "@hapi/joi",
    "CircleCi",
    "Docker",
    "Express",
    "Jest",
    "Morgan",
    "Nodemon",
    "Prettier",
    "husky",
    "supertest",
    "ESLint",
    "Mongoose"
  ],
  "author": "David De La Rosa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/D-Rosa99/Books_API/issues"
  },
  "homepage": "https://github.com/D-Rosa99/Books_API#readme",
  "dependencies": {
    "@babel/polyfill": "7.8.7",
    "@hapi/joi": "17.1.1",
    "@hapi/joi-date": "2.0.1",
    "config": "3.3.1",
    "express": "4.17.1",
    "mongoose": "5.9.7",
    "rimraf": "^3.0.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "7.9.5",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "6.8.0",
    "eslint-config-node": "4.0.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.5",
    "jest": "25.2.7",
    "morgan": "1.10.0",
    "nodemon": "2.0.2",
    "prettier": "2.0.4",
    "pretty-quick": "2.0.1",
    "supertest": "4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  }
}
