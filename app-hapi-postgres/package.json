{
  "name": "app-hapi-postgres",
  "version": "1.0.0",
  "description": "This is a project to train my knowledge of hapi and postgres",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log",
    "start": "npm run build && node dist/app/index.js",
    "start:dev": "nodemon --exec babel-node app/index.js",
    "lint": "eslint --ext js . --fix",
    "prettier": "prettier --write .",
    "test": "jest --verbose",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "Config",
    "Docker",
    "Eslint",
    "Hapi",
    "Husky",
    "Jest",
    "Morgan",
    "Nodemon",
    "Postgres",
    "Prettier",
    "Sequelize",
    "Winston"
  ],
  "author": "David De La Rosa",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@hapi/hapi": "19.1.1",
    "pg": "8.0.3",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "eslint": "6.8.0",
    "eslint-config-node": "4.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "morgan": "1.10.0",
    "nodemon": "2.0.3",
    "prettier": "2.0.5",
    "pretty-quick": "^2.0.1"
  }
}
