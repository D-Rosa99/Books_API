{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "prettier", "node"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier", "node"],
  "rules": {
    "no-console": 1
  }
}
